{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "rootDir": "./app/static/js",
    "outDir": "./app/static/dist",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "moduleResolution": "node"
  }
}